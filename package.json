{
  "name": "vuetils",
  "description": "Utils library for Vue",
  "version": "0.3.1",
  "sideEffects": false,
  "files": [
    "/dist"
  ],
  "main": "dist/vuetils.common.js",
  "keywords": [
    "vue",
    "utils"
  ],
  "author": "Antoine CARON",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Slashgear/vuetils.git"
  },
  "scripts": {
    "build": "vue-cli-service build --target lib --name vuetils src/index.js",
    "lint": "vue-cli-service lint",
